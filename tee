<script type="text/javascript">
    function closeLayer() {
        jQuery("#reserveGameCafeLayer").css("display" , "none");
        jQuery("#ios_radio")[0].checked = false;
        jQuery("#android_radio")[0].checked = false;
    }

    function addReservationGameCafe() {
        var welIosInput = jQuery("#ios_radio");
        var welAndroidInput = jQuery("#android_radio");

        if (!(welIosInput[0].checked ^ welAndroidInput[0].checked)) {
            alert("os를 선택해주세요.");
            return;
        }

        jQuery.ajax({
            url: '/PlugReserveGameCafeAjax.nhn',
            method: 'post',
            data: {
                'cafeId': '29066031',
                'os': welIosInput[0].checked ? welIosInput[0].value : welAndroidInput[0].value
            }
        }).done(onLoadReserve).fail(onErrorReserve);

        sendReservationCompleteBA('29066031');
    }

    function onLoadReserve(oRes) {
        var oMessage = oRes.message;

        if (!oMessage) {
            alert("사전예약 신청 중 문제가 발생했습니다. 잠시 후 다시 시도해주세요.");
            return;
        }

        if (oMessage.status !== '200') {
            alert(oMessage.error.msg);
            closeLayer();
            return;
        }

        alert("사전예약 참여가 완료되었습니다.");
        location.reload();
    }

    function onErrorReserve(oRes) {
        alert(oRes.message.error.msg);
    }

    function sendReservationCompleteBA(sCafeId) {
        oBAStatSender.send([
            {
                eventType : cafe.BAStatEventType.PC_1442_167,
                extra : {
                    cafe_id : sCafeId
                }
            }
        ]);
    }
</script>

