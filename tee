            redirectToCertPage();
		}
	}




function hideDenyNamelessLayer() {
	document.getElementById("denyNamelessLayer").style.display = "none";
}

function redirectToCertPage() {
	toCertPage(encodeURIComponent(document.location.href), encodeURIComponent('https://cafe.naver.com/korbuildit'));
}
</script>
